    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "sails": "^1.0.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/lodash": "^4.14.134",
    "mocha": "^6.1.4",
    "path-exists": "^4.0.0",
    "sails": "^1.0.0",
    "typescript": ">=2.4.2 <2.7.0"
  }
